{
	"scripts": {
		"postinstall": "lerna bootstrap",
		"clean": "lerna run --parallel clean",
		"run-proxy": "lerna run --parallel run-proxy",
		"start": "lerna run --parallel start",
		"build": "lerna run build",
		"docker:build": "docker build -t trial-management-tool .",
		"docker:tag": "docker tag trial-management-tool drivereu/trial-management-tool:2.0.8",
		"docker:tag:latest": "docker tag trial-management-tool drivereu/trial-management-tool:2.0.8",
		"docker:publish": "docker push drivereu/trial-management-tool:2.0.8",
		"docker:publish:latest": "docker push drivereu/trial-management-tool:2.0.8",
		"docker:latest": "npm run  docker:tag:latest && npm run docker:publish:latest",
		"docker": "npm run docker:local && npm run docker:publish && npm run docker:latest",
		"docker:local": "npm run docker:build && npm run docker:tag",
		"docker:run": "docker run --name tmt -it -p 3210:3210 trial-management-tool",
		"docker:sh": "docker run -it trial-management-tool sh"
	},
	"devDependencies": {
		"handlebars": "^4.7.3",
		"lerna": "^3.20.2"
	},
	"name": "trial-management-tool"
}