    "mithril-markdown": "^0.4.0",
    "mithril-materialized": "^0.17.1",
    "mithril-scenario-timeline": "^0.2.2",
    "mithril-table": "^0.2.2",
    "mithril-tree-component": "^0.7.1",
    "mithril-ui-form": "^0.8.14",
